@CHARSET "UTF-8";
.button2 {
	-moz-box-shadow: 2px 3px 4px 0px #888a8c;
	-webkit-box-shadow: 2px 3px 4px 0px #888a8c;
	box-shadow: 2px 3px 4px 0px #888a8c;
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0.05, #ffffff), color-stop(1, #e8e3e8));
	background:-moz-linear-gradient(top, #ffffff 5%, #e8e3e8 100%);
	background:-webkit-linear-gradient(top, #ffffff 5%, #e8e3e8 100%);
	background:-o-linear-gradient(top, #ffffff 5%, #e8e3e8 100%);
	background:-ms-linear-gradient(top, #ffffff 5%, #e8e3e8 100%);
	background:linear-gradient(to bottom, #ffffff 5%, #e8e3e8 100%);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffff', endColorstr='#e8e3e8',GradientType=0);
	background-color:#ffffff;
	-moz-border-radius:4px;
	-webkit-border-radius:4px;
	border-radius:4px;
	border:1px solid #F2F2F2;
	display:inline-block;
	cursor:pointer;
	color:#000000;
	font-size:14px;
	font-weight:bold;
	padding:5px 10px;
	text-decoration:none;
	text-shadow:0px 1px 0px #ffffff;
}
.button2:hover {
	/*background:-webkit-gradient(linear, left top, left bottom, color-stop(0.05, #e8e3e8), color-stop(1, #ffffff));
	background:-moz-linear-gradient(top, #e8e3e8 5%, #ffffff 100%);
	background:-webkit-linear-gradient(top, #e8e3e8 5%, #ffffff 100%);
	background:-o-linear-gradient(top, #e8e3e8 5%, #ffffff 100%);
	background:-ms-linear-gradient(top, #e8e3e8 5%, #ffffff 100%);
	background:linear-gradient(to bottom, #e8e3e8 5%, #ffffff 100%);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#e8e3e8', endColorstr='#ffffff',GradientType=0);
	background-color:#e8e3e8;*/
	text-decoration: none;
}
.button2:active {
	position:relative;
	top:1px;
	-moz-box-shadow: 1px 0px 4px 0px #888a8c;
	-webkit-box-shadow: 1px 0px 4px 0px #888a8c;
	box-shadow: 1px 0px 4px 0px #888a8c;
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0.05, #e8e3e8), color-stop(1, #ffffff));
	background:-moz-linear-gradient(top, #e8e3e8 5%, #ffffff 100%);
	background:-webkit-linear-gradient(top, #e8e3e8 5%, #ffffff 100%);
	background:-o-linear-gradient(top, #e8e3e8 5%, #ffffff 100%);
	background:-ms-linear-gradient(top, #e8e3e8 5%, #ffffff 100%);
	background:linear-gradient(to bottom, #e8e3e8 5%, #ffffff 100%);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#e8e3e8', endColorstr='#ffffff',GradientType=0);
	background-color:#e8e3e8;
}


