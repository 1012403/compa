@CHARSET "UTF-8";
.main_container {
	float: left; 
	width: 100%;
}
.upload_area_container {
	width: 100%;
}

.main_header_container {
	float: left;
	width: 100%;
}

.content_container{
	border-style: solid;
	border-width : 1px;
	border-color: #D4D0C8;
	float: left;
	width: 100%;
	margin-top: 20px;
}
.title_content{
	float: left;
	width: 100%;
	background-color: #E1E6F6;
}

.file_img_container{
	border-style: solid;
	border-color:  white;
	width: 200px;
	float: left;
	margin: 10px;
	height: 205px;
}

.file_img_container img {
	
}

.file_img_container  img{
	-moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    -khtml-border-radius: 5px;
    border-radius: 5px;
    box-shadow: 4px 4px 2px #888888;
    margin-bottom: 5px;
}

.image_name  {
	width: 90%;
	text-align: center;
}

.img_selected{
	border-style: double;
	border-color: red;
	-moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    -khtml-border-radius: 5px;
    border-radius: 5px;
}

.title_selected{
	float: left;
	width: 100%;
	background-color: #E1E6F6;
	margin-top: 10px;
}

.cls_selected_img {
	-moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    -khtml-border-radius: 5px;
    border-radius: 5px;
    margin: 5px;
    box-shadow: 10px 10px 5px #888888;
}

.inner_image {
	margin: 2px;
}

.div_select_img a{
	display:none;
	position:absolute;
	z-index: 99;
	width: 100px;
	height: 25px;
	/*-moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    -khtml-border-radius: 5px;
    border-radius: 5px;*/
    background-color: #74B960;
    text-align: center;
    padding-top: 5px;
    -webkit-box-shadow: inset 0px -3px 1px rgba(0, 0, 0, 0.45), 0px 2px 2px rgba(0, 0, 0, 0.25);
    -moz-box-shadow: inset 0px -3px 1px rgba(0, 0, 0, 0.45), 0px 2px 2px rgba(0, 0, 0, 0.25);
    box-shadow: inset 0px -3px 1px rgba(0, 0, 0, 0.45), 0px 2px 2px rgba(0, 0, 0, 0.25);
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    text-shadow: 1px 1px 0px rgba(255, 255, 255, 0.5);
    color: #141F2D;
    font-weight: bold;
    text-decoration:none;
}

.div_select_img a:active{
	position:absolute;
	z-index: 99;
	width: 100px;
	height: 25px;
	/*-moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    -khtml-border-radius: 5px;
    border-radius: 5px;*/
    background-color: #74B960;
    text-align: center;
    padding-top: 5px;
   -webkit-box-shadow: inset 0px -3px 1px rgba(149, 214, 124, 1), inset 0 0px 3px rgba(0, 0, 0, 0.9);
    -moz-box-shadow: inset 0px -3px 1px rgba(149, 214, 124, 1), inset 0 0px 3px rgba(0, 0, 0, 0.9);
    box-shadow: inset 0px -3px 1px rgba(149, 214, 124, 1), inset 0 0px 3px rgba(0, 0, 0, 0.9);
}