@import url("css/bootstrap.min.css");

body {
  font-family: "ＭＳ Ｐゴシック";
  font-size: 13px;
}

.header{
  padding: 3px;
}

.rightcolumn{
  display: block;
  float: left;
  width: 100%;
  padding-left:2px;
}

.header a{
  -webkit-border-radius: 3px 3px 3px 3px;
  border-radius: 3px 3px 3px 3px;
  -webkit-box-shadow: 2px 2px 1px 2px #8C8C8C;
  box-shadow: 2px 2px 1px 2px #8C8C8C;

  float: right;
  padding: 3px;
  margin-left: 2px;
  margin-top: 5px;
  margin-right: 5px;
  color: black;
}

.header .name{
  font-weight: bold;
}
.header .order .name{
  color: #e71b7c;
}
.header .quotes .name{
  color: #e55104;
}
.navbar{
  margin-bottom: 1px;
}
.navbar-inverse2 {
  border: 0px;
  border-radius: 0;
  border-bottom: 2px solid #0088fc;

background: rgb(254,255,255);  Old browsers
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top,  rgba(254,255,255,1) 0%, rgba(242,245,247,1) 63%, rgba(221,241,249,1) 63%, rgba(221,241,249,1) 63%, rgba(225,232,238,1) 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(254,255,255,1)), color-stop(63%,rgba(242,245,247,1)), color-stop(63%,rgba(221,241,249,1)), color-stop(63%,rgba(221,241,249,1)), color-stop(100%,rgba(225,232,238,1))); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  rgba(254,255,255,1) 0%,rgba(242,245,247,1) 63%,rgba(221,241,249,1) 63%,rgba(221,241,249,1) 63%,rgba(225,232,238,1) 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  rgba(254,255,255,1) 0%,rgba(242,245,247,1) 63%,rgba(221,241,249,1) 63%,rgba(221,241,249,1) 63%,rgba(225,232,238,1) 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  rgba(254,255,255,1) 0%,rgba(242,245,247,1) 63%,rgba(221,241,249,1) 63%,rgba(221,241,249,1) 63%,rgba(225,232,238,1) 100%); /* IE10+ */
background: linear-gradient(to bottom,  rgba(254,255,255,1) 0%,rgba(242,245,247,1) 63%,rgba(221,241,249,1) 63%,rgba(221,241,249,1) 63%,rgba(225,232,238,1) 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#feffff', endColorstr='#e1e8ee',GradientType=0 ); /* IE6-8 */
}

.navbar-inverse {
  border: 0px;
  border-radius: 0;
  border-bottom: 0px solid #0088fc;
  background-image:url('../images/web/bg_menu_1.png');
/* 	background-repeat:repeat-x; */
  margin-bottom: 5px;

}

.navbar-nav a {
  font-weight: bold;
  color: black !important;
}
.navbar-nav a:hover {
  color: #0088fc !important;
}
.navbar-toggle{
background-color: #333;
margin-right: 5px;
margin-left: 5px;
}

/* form search */

#keyword{
    /* The search text box. */
    color:#888888;
    background:url("../images/search_icon.png") no-repeat;
  width: 138px;
    height:27px;
    line-height:27px;
    padding:0 0 0 20px;
    text-shadow:1px 1px 0 white;
    margin-top: 5px;
    margin-left: -10px;
}

#submitSearch{
  -webkit-border-radius: 6;
  -moz-border-radius: 6;
  border-radius: 6px;
  -webkit-box-shadow: 2px 2px 4px #666666;
  -moz-box-shadow: 2px 2px 4px #666666;
  box-shadow: 2px 2px 4px #666666;
  color: #558ED5;
  padding: 3px 10px 2px 10px;
  text-decoration: none;
  font-weight: bold;
  background: white;
  border-width: 0px;
  height: 25px;
}

#sidebar input.date{
  width: 78px;
  height: 27px;
}
input[type=date]::-webkit-inner-spin-button,
input[type=date]::-webkit-outer-spin-button {
    -webkit-appearance: none;
    margin: 0;
}
#sidebar h5 b{
  display: block;
  height:23px;
  line-height:23px;
  background-image: url("../images/web/circle.png ");
  background-repeat: no-repeat;
  background-position: left center;
  background-size: 18px 18px;
  padding: 0 0 0 22px;
}
.condition{
  padding: 10px 5px 3px 0px;
}
.condition b{
  padding: 0px 0px 0px 20px;
}
.condition table{
  margin-left: 5px;
  width: 190px;
}
.condition .parent{
  background-color: blue;
  color: white;
  font-weight: bold;
  border: 2px solid blue;
}
.condition .parent .name{
  padding: 3px 5px 3px 15px;
}
.condition .parent .delete input{
  color: black;
}
.condition .child{
  padding: 5px 5px 5px 25px;
  border: 2px solid blue;
  font-weight: bold;
}
/* sidebar */

#sidebar h5{
  padding: 2px 2px 2px 10px;
  margin-left: 5px;
  -webkit-border-radius: 2px 2px 2px 2px;
  border-radius: 2px 2px 2px 2px;
  -webkit-box-shadow: 1px 1px 1px 1px #8C8C8C;
  box-shadow: 1px 1px 1px 1px #8C8C8C;
  margin-right: 7px;
}
#sidebar ul {
  list-style: none;
  padding: 2px 5px 2px 5px;
}
#sidebar>ul>li>a{
  text-decoration: none;
  padding: 5px 5px 5px 15px;
  display: block;
  background-color: blue;
  color: white;
  font-weight: bold;
  margin-bottom: 3px;
}

#sidebar>ul>li>ul>li>a{
  text-decoration: underline;
  padding: 1px 1px 1px 25px;
  display: block;
  font-weight: bold;
  margin-bottom: 3px;
}

#listproduce{
  font-size: small !important;
}

#listproduce .row {
  margin: 0;
}
#listproduce .row .row {
  padding-left: 15px;
  padding-right: 15px;
}

#listproduce .row>div{
  padding: 10px;
}

#listproduce .table {
  margin-bottom: 0;
}
#listproduce .table>tbody>tr>td {
  border-top: 0;
}

#listproduce [class*="col-md-"] {
  padding-left: 0;
  padding-right: 0;
  padding-top: 15px;
  padding-bottom: 15px;
}
#listproduce .price span{
  display: block;
  clear: both !important;
}
#listproduce .number{
  padding-left: 20px;
}
#listproduce .end{
  margin-top: 20px;
}

#listproduce input{
  height: 25px;
  width: 150px;
}
#listproduce img.product{
  max-width: 100px;
}
#listproduce .button, .button{
  text-align: center;
  display: inline-block;javascript:void(0);
  margin-top: 5px;
  padding: 3px 5px 3px 5px;
  -webkit-border-radius: 2px 2px 2px 2px;
  border-radius: 2px 2px 2px 2px;
  -webkit-box-shadow: 1px 1px 1px 1px #8C8C8C;
  box-shadow: 1px 1px 1px 1px #8C8C8C;
}

.button:active{
  border-style:solid;
  border-width:1px;
  box-shadow: 1px 1px 0px 0px #8C8C8C;
}

#listproduce .star{
  background-color: #ffff99;
  color: #984807;
  font-weight: bold;
  margin-left: 5px;
  text-decoration: none;
}

#listproduce .nonstar{
  background-color: #fff;
  color: #000;
  font-weight: bold;
  margin-left: 5px;
  text-decoration: none;
}

#listproduce .order{
  clear: both;
  color: #e71b7c;
  width: 140px;
}
#listproduce .quotes{
  color: #e55104;
  width: 140px;
}

hr {
  margin-top: 0px;
  margin-bottom: 0px;
  border-top: 2px solid #eee;
}

.img-responsive {
  display: inline-block;
}



.title_tyle{
  background-image: url("../images/web/title_bg_5.png ");
  background-repeat: repeat-x;
  height: 40px;
  line-height:40px;
  text-align: right;

  -webkit-border-radius: 5px;
  border-radius: 7px;
}
.title_tyle label.pull-left{
  background-image: url("../images/web/circle.png ");
  background-repeat: no-repeat;
  background-position: left center;
  background-size: 18px 18px;
  padding: 0 0 0 22px;
}
.title_col{
  padding-top: 0px !important;
  padding-bottom: 0px !important;
}
.title_col .pagination{
  margin: 5px;
}

.sortbutton{
  -webkit-border-radius: 5px 5px 5px 5px;
  border-radius: 5px 5px 5px 5px;
  -webkit-box-shadow: 0px 0px 1px 0px gray;
  box-shadow: 0px 0px 1px 0px gray;
  background: #FFFFFF;
}
.sortinsetbutton{
  -webkit-border-radius: 5px 5px 5px 5px;
  border-radius: 5px 5px 5px 5px;
  -webkit-box-shadow:inset 3px 3px 5px 1px #9D70A6;
  box-shadow:inset 3px 3px 5px 1px #9D70A6;
  background: #FFCCFF;
}
.title_tyle a{
  margin: -15px 5px 0 0;
  padding: 7px 10px 7px 10px;
  text-decoration: none;
  color: gray;
  font-weight: bold;
}
.title_tyle select{
  height: 23px;
  line-height: 23px;
}
.title_tyle option{
  padding: 5px;
}


.navbar-inverse, .navbar-inverse .navbar-form {
  /* background-color: #F5F5F7; */
  border: 0px;
  border-radius: 0;  
  background-image:url('../images/web/bg_menu_1.png') no-repeat;
  margin-top: 10px;

}
.navbar-inverse .navbar-collapse,.navbar-inverse .navbar-form{
  border-color:#fff
}

table {
/* border-collapse: collapse; */
    border-spacing: 0;
}

